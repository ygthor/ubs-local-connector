{
    "name": "hisamu/php-xbase",
    "type": "library",
    "description": "A simple parser for *.dbf, *.fpt files using PHP",
    "keywords": ["dbase", "dbf", "foxpro"],
    "homepage": "https://github.com/luads/php-xbase",
    "license": "MIT",
    "authors": [
        {
            "name": "LuÃ£ de Souza",
            "email": "lsouza@codeinc.com.br"
        },
        {
            "name": "Erwin Kooi",
            "email": "info@cyane.nl"
        },
        {
            "name": "Alexander Strizhak",
            "email": "gam6itko@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-4": {
            "XBase\\": "src"
        },
        "exclude-from-classmap": ["/tests"]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0||^8.0||^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "XBase\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
